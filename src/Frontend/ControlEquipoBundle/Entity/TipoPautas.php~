<?php

namespace Frontend\ControlEquipoBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * TipoPautas
 *
 * @ORM\Table(name="controlequipo.tipo_pautas")
 * @ORM\Entity
 */
class TipoPautas
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="SEQUENCE")
     * @ORM\SequenceGenerator(sequenceName="controlequipo.tipo_pautas_id_seq", allocationSize=1, initialValue=1)
     */
    private $id;

    /**
     * @var string
     *
     * @ORM\Column(name="descripcion_solicitud", type="string", length=50, nullable=false)
     */
    private $descripcionSolicitud;


}
