{% extends 'EstudioCabinaBundle::template.html.twig' %}

{% block body -%}
    <h1>HistorialReservaciones</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Usuario</th>
                <td>{{ entity.usuario }}</td>
            </tr>
            <tr>
                <th>Fechahora</th>
                <td>{{ entity.fechaHora|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Estatus</th>
                <td>{{ entity.estatus }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('historialreservaciones') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('historialreservaciones_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
